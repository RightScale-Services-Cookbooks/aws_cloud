{
    
    "vagrant": {
        "box_name": "nat - host"
    },
    "aws": {"vpc_nat": {
            "nat_ha": "enabled",
            "java_home": "/etc/alternatives/jre",
            "aws_account_secret": "foo",
            "aws_account_id": "bar"
        },
        "ses": {
            "domain": "example.com",
            "virtual_alias_domains": ["example.com"],
            "servers": "ses.amazonaws.com",
            "username": "foobar",
            "password": "password"
        }
    },
    "run_list": [
        "recipe[aws::vpc-nat]",
        "recipe[aws::vpc-nat-ha]"
    ]
}